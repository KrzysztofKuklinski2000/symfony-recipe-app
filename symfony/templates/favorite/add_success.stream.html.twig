<turbo-stream action="update" target="favorite-btn-{{ recipe.id }}">
    <template>
        {% include "favorite/_btn.html.twig" with { recipe: recipe } %}
    </template>
</turbo-stream>

<turbo-stream action="update" target="favorite-count-{{ recipe.id }}">
    <template>
        {{ recipe.favoritedBy|length }}
    </template>
</turbo-stream>
